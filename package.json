{
  "name": "elitemo",
  "version": "1.0.0",
  "description": "automobile service",
  "main": "index.js",
  "scripts": {
    "start": "doppler run --command='NODE_PATH=./build node build/index.js'",
    "build": "tsc",
    "start:dev": "doppler run --command='tsc-watch --noClear --onSuccess \"node --inspect=0.0.0.0:9229 ./build/index.js\"'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "code:format": "prettier './src/**/**.{js,jsx,ts,tsx}'",
    "code:check:rules": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "prepare": "husky install"
  },
  "keywords": [
    "automobile"
  ],
  "author": "okandeji",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.30.1",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "ioredis": "^5.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.0",
    "mongoose-paginate-v2": "^1.7.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.1.20",
    "path": "^0.12.7",
    "winston": "^3.8.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose-paginate-v2": "^1.6.5",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "poetic": "^1.7.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
